package PatternCases;

public class Sizepattern {

public static void main(String [] args)	{
	
	
	for(int k=0; k<=4 ;k++) {
		
	for(int h=0; h<=k; k++) {
		
	
	System.out.print("h"+" ");		
	}
	
	System.out.println("");
	
	
	for (int d=0;d<=4;d++) {
		
	}
	
	for(int j=0; j<=4;j++) {
		
	System.out.println("8" +" ");
	System.out.println("");
	}
	}
}
	
}
